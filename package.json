    "@mui/material": "^6.1.6",
    "@mui/utils": "^6.1.6",
    "@playwright/mcp": "^0.0.46",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "accessflow-mcp-server": "^1.0.6",
    "dompurify": "^3.2.3",
    "framer-motion": "^11.15.0",
    "prismjs": "^1.30.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-code-blocks": "^0.1.6",
    "react-code-section-lib": "^0.1.2",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.1",
    "react-scripts": "^5.0.1",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@acsbe/accessflow-sdk": "file:acsbe-accessflow-sdk-1.0.0.tgz",
