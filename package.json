    "playwright": "^1.56.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "predeploy": "npm run build",
    "deploy": "bash scripts/deploy-via-actions.sh",
    "deploy:direct": "gh-pages -d build",
    "deploy:actions": "bash scripts/deploy-via-actions.sh",
    "deploy:trigger": "gh workflow run deploy.yml",
    "build": "CI=false react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "test:e2e": "cd test-suite && playwright test",
    "test:e2e:ui": "cd test-suite && playwright test --ui",
    "test:e2e:headed": "cd test-suite && playwright test --headed",
    "test:e2e:debug": "cd test-suite && playwright test --debug",
    "test:e2e:report": "cd test-suite && playwright show-report",
    "test:e2e:search": "cd test-suite && playwright test tests/search.spec.js",
    "test:e2e:accessibility": "cd test-suite && playwright test tests/accessibility-suite.spec.js",
    "test:e2e:keyboard": "cd test-suite && playwright test tests/keyboard-navigation.spec.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
